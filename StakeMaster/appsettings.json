{
	"TransactionSettings": {
		"InputSize": 148,
		"OutputSize": 34,
		"Overhead": 10,
		"FreeTransactionSizeLimit": 1000,
		"RpcTimeoutInSeconds": 60,
		"BaseDateOfTransactions": "1970-01-01"
	},
	"Serilog": {
			"Using": [ "Serilog.Sinks.Console" ],
			"MinimumLevel": "Verbose",
			"Enrich": [ "WithAssemblyName", "WithThreadId" ],
			"WriteTo:Async": {
				"Name": "Async",
				"Args": {
					"configure": [
						{
							"Name": "Console",
							"Args": {
								"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss}: [{Level:u3} {ThreadId}] {AssemblyName} - {Message}{NewLine}{Exception}",
								"theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Literate, Serilog.Sinks.Console"
							}
						}
					],
					"blockWhenFull": true
				}
			}
		}
	}
